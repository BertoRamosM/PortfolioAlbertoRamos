---
import AstroIcon from '../icons/AstroIcon.astro'
import CssIcon from '../icons/CssIcon.astro'
import GithubIcon from '../icons/Github.astro'
import GitIcon from '../icons/GitIcon.astro'
import HtmlIcon from '../icons/HtmlIcon.astro'
import JavaScriptIcon from '../icons/JavaScriptIcon.astro'
import ReactIcon from '../icons/ReactIcon.astro'
import SaasIcon from '../icons/SaasIcon.astro'
import StyledComponentsIcon from '../icons/StyledComponentsIcon.astro'
import TailwindIcon from '../icons/TailwindIcon.astro'
import TypeScriptIcon from '../icons/TypeScript.astro'

const ICONS =[
  HtmlIcon,
  CssIcon,
  JavaScriptIcon,
  ReactIcon,
  AstroIcon,
  TypeScriptIcon,
  TailwindIcon,
  SaasIcon,
  StyledComponentsIcon,
  GithubIcon,
  GitIcon
]
---

<div class="w-full flex justify-center mt-9">
  <div class="flex flex-wrap justify-center max-w-screen-lg gap-x-10 gap-y-2 pb-12">
    {ICONS.map((Icon) => (
      <div class="m-2">
        <Icon class="w-12 h-12" />
      </div>
    ))}
  </div>
</div>
